{
  "info": {
    "name": "GLaDOS - FOD Detection API",
    "description": "Manual testing collection for the GLaDOS Aperture Labs FOD Detection API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    { "key": "base_url", "value": "http://127.0.0.1:8000" },
    { "key": "user_id", "value": "" },
    { "key": "project_id", "value": "" },
    { "key": "submission_id", "value": "" },
    { "key": "anomaly_id", "value": "" },
    { "key": "member_user_id", "value": "" }
  ],
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const res = pm.response.json();",
                  "if (res.success && res.user) {",
                  "  pm.collectionVariables.set('user_id', res.user.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/auth/login",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"alice@example.com\",\n  \"password\": \"password123\"\n}"
            }
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "header": [],
            "url": "{{base_url}}/auth/logout"
          }
        }
      ]
    },
    {
      "name": "Users",
      "item": [
        {
          "name": "Create User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const res = pm.response.json();",
                  "if (res.id) {",
                  "  pm.collectionVariables.set('user_id', res.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/users",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"testuser@example.com\",\n  \"password\": \"password123\"\n}"
            }
          }
        },
        {
          "name": "Get User",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{base_url}}/users/{{user_id}}"
          }
        },
        {
          "name": "Update User",
          "request": {
            "method": "PATCH",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/users/{{user_id}}",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"updated@example.com\"\n}"
            }
          }
        },
        {
          "name": "Delete User",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": "{{base_url}}/users/{{user_id}}"
          }
        }
      ]
    },
    {
      "name": "Projects",
      "item": [
        {
          "name": "Create Project",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const res = pm.response.json();",
                  "if (res.id) {",
                  "  pm.collectionVariables.set('project_id', res.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/projects",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Test Inspection Project\",\n  \"description\": \"A test project for FOD detection\"\n}"
            }
          }
        },
        {
          "name": "List Projects",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/projects",
              "host": ["{{base_url}}"],
              "path": ["projects"],
              "query": [
                { "key": "include_archived", "value": "false", "disabled": true }
              ]
            }
          }
        },
        {
          "name": "List Projects (Include Archived)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/projects?include_archived=true",
              "host": ["{{base_url}}"],
              "path": ["projects"],
              "query": [
                { "key": "include_archived", "value": "true" }
              ]
            }
          }
        },
        {
          "name": "Get Project",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}"
          }
        },
        {
          "name": "Update Project",
          "request": {
            "method": "PATCH",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/projects/{{project_id}}",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Project Name\",\n  \"description\": \"Updated description\"\n}"
            }
          }
        },
        {
          "name": "Archive Project",
          "request": {
            "method": "POST",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}/archive"
          }
        },
        {
          "name": "Delete Project",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}"
          }
        }
      ]
    },
    {
      "name": "Project Members",
      "item": [
        {
          "name": "Add Member",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const res = pm.response.json();",
                  "if (res.user_id) {",
                  "  pm.collectionVariables.set('member_user_id', res.user_id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/projects/{{project_id}}/members",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"project_id\": \"{{project_id}}\",\n  \"user_id\": \"22222222-2222-2222-2222-222222222222\",\n  \"role\": \"editor\"\n}"
            }
          }
        },
        {
          "name": "List Members",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}/members"
          }
        },
        {
          "name": "Get Member",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}/members/{{member_user_id}}"
          }
        },
        {
          "name": "Update Member Role",
          "request": {
            "method": "PATCH",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/projects/{{project_id}}/members/{{member_user_id}}",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"role\": \"viewer\"\n}"
            }
          }
        },
        {
          "name": "Remove Member",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}/members/{{member_user_id}}"
          }
        }
      ]
    },
    {
      "name": "Submissions",
      "item": [
        {
          "name": "Create Submission",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const res = pm.response.json();",
                  "if (res.id) {",
                  "  pm.collectionVariables.set('submission_id', res.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/projects/{{project_id}}/submissions",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"project_id\": \"{{project_id}}\",\n  \"image_id\": \"dddddddd-dddd-dddd-dddd-ddddddddd001\",\n  \"submitted_by_user_id\": \"11111111-1111-1111-1111-111111111111\"\n}"
            }
          }
        },
        {
          "name": "List Submissions",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/projects/{{project_id}}/submissions",
              "host": ["{{base_url}}"],
              "path": ["projects", "{{project_id}}", "submissions"],
              "query": [
                { "key": "status", "value": "queued", "disabled": true },
                { "key": "pass_fail", "value": "fail", "disabled": true }
              ]
            }
          }
        },
        {
          "name": "Get Submission",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}/submissions/{{submission_id}}"
          }
        },
        {
          "name": "Update Submission",
          "request": {
            "method": "PATCH",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/projects/{{project_id}}/submissions/{{submission_id}}",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"complete\",\n  \"pass_fail\": \"pass\",\n  \"anomaly_count\": 0\n}"
            }
          }
        },
        {
          "name": "Retry Submission",
          "request": {
            "method": "POST",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}/submissions/{{submission_id}}/retry"
          }
        },
        {
          "name": "Delete Submission",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": "{{base_url}}/projects/{{project_id}}/submissions/{{submission_id}}"
          }
        }
      ]
    },
    {
      "name": "Anomalies",
      "item": [
        {
          "name": "Create Anomaly",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const res = pm.response.json();",
                  "if (res.id) {",
                  "  pm.collectionVariables.set('anomaly_id', res.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/anomalies",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"submission_id\": \"{{submission_id}}\",\n  \"label\": \"scratch\",\n  \"description\": \"Visible scratch on surface\",\n  \"severity\": \"med\",\n  \"confidence\": 0.92\n}"
            }
          }
        },
        {
          "name": "List Anomalies for Submission",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/anomalies?submission_id={{submission_id}}",
              "host": ["{{base_url}}"],
              "path": ["anomalies"],
              "query": [
                { "key": "submission_id", "value": "{{submission_id}}" },
                { "key": "severity", "value": "high", "disabled": true }
              ]
            }
          }
        },
        {
          "name": "Get Anomaly",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{base_url}}/anomalies/{{anomaly_id}}"
          }
        },
        {
          "name": "Update Anomaly",
          "request": {
            "method": "PATCH",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{base_url}}/anomalies/{{anomaly_id}}",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"severity\": \"high\",\n  \"confidence\": 0.97\n}"
            }
          }
        },
        {
          "name": "Acknowledge Anomaly",
          "request": {
            "method": "POST",
            "header": [],
            "url": "{{base_url}}/anomalies/{{anomaly_id}}/acknowledge"
          }
        },
        {
          "name": "Delete Anomaly",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": "{{base_url}}/anomalies/{{anomaly_id}}"
          }
        }
      ]
    },
    {
      "name": "Uploads",
      "item": [
        {
          "name": "Upload Design File",
          "request": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{base_url}}/uploads/design?project_id={{project_id}}",
              "host": ["{{base_url}}"],
              "path": ["uploads", "design"],
              "query": [
                { "key": "project_id", "value": "{{project_id}}" }
              ]
            },
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "",
                  "description": "PDF or TXT design file"
                }
              ]
            }
          }
        },
        {
          "name": "Upload Image",
          "request": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{base_url}}/uploads/image?project_id={{project_id}}",
              "host": ["{{base_url}}"],
              "path": ["uploads", "image"],
              "query": [
                { "key": "project_id", "value": "{{project_id}}" }
              ]
            },
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "",
                  "description": "PNG or JPEG image file"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Detection",
      "item": [
        {
          "name": "Receive Detection Result (Webhook)",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "x-webhook-secret", "value": "your-webhook-secret-here" }
            ],
            "url": "{{base_url}}/webhooks/detection",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"response\": \"Detection complete\",\n  \"model\": \"detector-v1.2.0\",\n  \"inference_time_ms\": 342.5\n}"
            }
          }
        }
      ]
    },
    {
      "name": "Health",
      "item": [
        {
          "name": "Root",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{base_url}}/"
          }
        }
      ]
    }
  ]
}
